    "@tiptap/core": "^2.0.0",
    "@tiptap/pm": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasmandelli/tiptap-extension-indent.git",
    "directory": "packages/extension-indent"
