{
  "name": "@mandelli/tiptap-extension-indent",
  "description": "indent extension for tiptap",
  "version": "0.0.1",
  "homepage": "https://tiptap.dev",
  "keywords": [
    "tiptap",
    "tiptap extension"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
